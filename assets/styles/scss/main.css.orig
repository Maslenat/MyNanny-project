.header__inner {
  display: flex;
  justify-content: space-between;
  align-items: center;
  gap: 10px;
}

.header__img {
  width: 240px;
  height: auto;
}

.header__menu {
  display: flex;
  align-items: center;
  gap: 15px;
}

.header__item {
  font-size: 14px;
  text-decoration: none;
}
.header__item:hover {
  cursor: pointer;
}

.header__page {
  margin-right: 12px;
  color: #292f36;
}
.header__page:hover {
  padding-bottom: 3px;
  border-bottom: 2px solid #4dcdc4;
}

.header__page_active {
  padding-bottom: 3px;
  border-bottom: 2px solid #4dcdc4;
}

.header__buttons {
  margin-right: 5px;
}

.header__check {
  display: none;
}

@media screen and (max-width: 1238px) {
  .header__img {
    width: 151px;
  }

  .mobile__hidden {
    display: none;
  }

  .header__menu {
    position: relative;
  }

  .header__img {
    z-index: 4;
    position: relative;
  }

  .header__btn {
    display: flex;
    align-items: center;
    right: 20px;
    width: 26px;
    height: 26px;
    cursor: pointer;
    z-index: 4;
  }

  .header__btn > span,
.header__btn > span::before,
.header__btn > span::after {
    display: block;
    position: absolute;
    width: 100%;
    height: 3px;
    background-color: #292f36;
  }

  .header__btn > span::before {
    content: "";
    top: -8px;
  }

  .header__btn > span::after {
    content: "";
    top: 8px;
  }

  .header__links {
    position: fixed;
    visibility: hidden;
    top: 0;
    left: -100%;
    width: 100%;
    height: 100%;
    margin: 0;
    padding: 130px 0;
    list-style: none;
    text-align: center;
    background-color: #ebebeb;
    box-shadow: 1px 0px 6px rgba(0, 0, 0, 0.2);
    z-index: 3;
    gap: 0;
  }
  .header__links:hover {
    text-decoration: none;
  }
}
@media screen and (max-width: 1238px) and (max-width: 375px) {
  .header__links {
    padding: 86px 0;
  }
}
@media screen and (max-width: 1238px) and (max-width: 1024px) and (max-height: 600px) {
  .header__links {
    padding: 75px 0;
  }
}
@media screen and (max-width: 1238px) and (max-width: 540px) and (max-height: 720px) {
  .header__links {
    padding: 75px 0;
  }
}
@media screen and (max-width: 1238px) {
  .header__item {
    display: block;
    color: #292f36;
    font-size: 22px;
    font-weight: 400;
    text-decoration: none;
  }
  .header__item:hover {
    color: #4dcdc4;
  }
}
@media screen and (max-width: 1238px) {
  .header__page {
    margin: 0;
    padding: 10px;
    border-bottom: none;
  }
  .header__page:hover {
    padding: 10px;
    border-bottom: none;
    text-decoration: none;
  }
}
@media screen and (max-width: 1238px) {
  .header__buttons {
    margin: 0;
    padding: 7px;
    background-color: transparent;
  }
  .header__buttons:hover {
    color: #4dcdc4;
    background: transparent;
  }
}
@media screen and (max-width: 1238px) {
  .header__check:checked ~ .header__btn > span {
    transform: rotate(45deg);
  }
}
@media screen and (max-width: 1238px) {
  .header__check:checked ~ .header__btn > span::before {
    top: 0;
    transform: rotate(0deg);
  }
}
@media screen and (max-width: 1238px) {
  .header__check:checked ~ .header__btn > span::after {
    top: 0;
    transform: rotate(90deg);
  }
}
@media screen and (max-width: 1238px) {
  .header__check:checked ~ .header__links {
    z-index: 3;
    visibility: visible;
    left: 0;
    display: flex;
    flex-direction: column;
    justify-content: center;
    margin-top: -50px;
    display: flex;
    align-items: center;
    justify-content: space-around;
  }
}
@media screen and (max-width: 1238px) {
  .header__btn > span,
.header__btn > span::before,
.header__btn > span::after {
    transition-duration: 0.5s;
  }
}
@media screen and (max-width: 1238px) {
  .header__item {
    transition-duration: 0.5s;
  }
}
@media screen and (max-width: 1238px) {
  .header__item {
    transition-duration: 0.5s;
  }
}
