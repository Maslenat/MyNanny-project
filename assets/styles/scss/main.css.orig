.btn {
  display: inline-block;
  box-sizing: border-box;
  color: #ffffff;
  font-size: 1.1rem;
  line-height: 1.55;
  font-weight: 600;
  letter-spacing: 0.5px;
  text-decoration: none;
  text-align: center;
  padding: 1.1rem 3.4rem 1.1rem 3.4rem;
  border: none;
}
.btn:hover {
  cursor: pointer;
  transition: background-color 0.4s ease;
}
.btn_defaultcolor {
  background: #4dcdc4;
}
.btn_defaultcolor:hover {
  background: #2baba0;
}
.btn_accentСolor {
